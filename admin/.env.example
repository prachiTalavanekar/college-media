# Admin Panel Environment Configuration Example
# Copy this file to .env and fill in your actual values

# Admin Credentials
ADMIN_EMAIL=your-admin@email.com
ADMIN_PASSWORD=your-secure-password

# Admin Panel Settings
ADMIN_PANEL_NAME=CampusConnect Admin Panel
ADMIN_SESSION_TIMEOUT=24h

# API Configuration
API_BASE_URL=http://localhost:5000/api
CLIENT_URL=http://localhost:3000

# Security
ADMIN_JWT_SECRET=your-admin-jwt-secret-key
ADMIN_RATE_LIMIT=100

# Email Notifications for Admin
ADMIN_NOTIFICATION_EMAIL=admin@yourdomain.com

# Database (Reference only - actual connection in server)
DB_NAME=collegeconnect

# Feature Flags
ENABLE_BULK_ACTIONS=true
ENABLE_EMAIL_NOTIFICATIONS=true
ENABLE_USER_EXPORT=true
ENABLE_SYSTEM_LOGS=true
